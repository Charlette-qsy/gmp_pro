{
    
    "clangd.fallbackFlags": [
        "-I${workspaceFolder}",
        "-I${workspaceFolder}/csp/example",
        "-I${workspaceFolder}/third_party/iqmath",
        "-I${workspaceFolder}/user/example"
    ],

    "clangd.enableCodeCompletion": true,

    "clangd.arguments": [
        "--background-index",
        "-j=12",
        "--all-scopes-completion",
    ],
    "files.associations": {
        "*.tcc": "cpp",
        "optional": "cpp",
        "ratio": "cpp",
        "system_error": "cpp",
        "array": "cpp",
        "functional": "cpp",
        "tuple": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "format": "cpp"
    }
}